<div class="project-list">
    {% for project of projects %}
        <a class="project-card" href="#">
            <span class="name">
                {{project.name}}
            </span>
        </a>
    {% endfor %}
    <a class="project-card project-card-create" href="{{createProjectURL}}">
        <div class="create-icon"></div>
    </a>
</div>